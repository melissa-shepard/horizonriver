        <fullName>Account_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   RecordTypeId  = &apos;012V00000008vOW&apos;,
  Not(ISCLONE()  ),
  Not(ISNEW() ), 
  ISCHANGED(AccountId) 
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Account Name con&apos;t be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordTypeId = &apos;012V00000008vOW&apos;, 
ISBLANK(AccountId )
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Account Name is required</errorMessage>
    </validationRules>
