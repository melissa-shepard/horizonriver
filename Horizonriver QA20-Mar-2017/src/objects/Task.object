    <validationRules>
        <fullName>Stage_1_Telecom_Team</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(HRT_Stage__c, &apos;1 - Telecom Team&apos;), 
ISPICKVAL(Status , &apos;Completed&apos;) , 
OR( 
ISBLANK(HRT_Circuit_10__c), 
ISBLANK(HRT_Carrier__c), 
ISBLANK(HRT_Circuit_Speed__c), 
ISBLANK(HRT_Circuit_Type__c), 
ISBLANK(HRT_Support_Pin__c) , 
ISNULL(HRT_Install_Date__c) , 
ISNULL(HRT_Modem_Validation_Date__c) 
) 
)</errorConditionFormula>
        <errorMessage>Following fields valus are required: Circuit 10, 
Carrier, 
Circuit Speed, 
Circuit Type, 
Install Date, 
Support Pin, 
Modem Validation Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_2_Staging_Team</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(HRT_Stage__c, &apos;2 - Staging Team&apos;), 
ISPICKVAL(Status , &apos;Completed&apos;) , 
OR( 
ISBLANK(HRT_HW_Serial__c), 
ISBLANK(HRT_HW_Name__c), 
ISBLANK(HRT_CP_MAC__c), 
ISBLANK(HRT_CP_IMEI__c), 
ISBLANK(HRT_VZ_CARD__c) , 
ISBLANK(HRT_HW_MAC__c) 
) 
)</errorConditionFormula>
        <errorMessage>Following fields valus are required: HW Serial #, 
HW Name, 
CP MAC, 
CP IMEI, 
VZ CARD #, 
HW MAC</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_3_Field_Team</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(HRT_Stage__c, &apos;3 - Field Team&apos;), 
ISPICKVAL(Status , &apos;Completed&apos;) , 
ISNULL(HRT_P4_Install_Date__c) 
)</errorConditionFormula>
        <errorDisplayField>HRT_P4_Install_Date__c</errorDisplayField>
        <errorMessage>P4 Install Date is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_4_CS_Tech_Sub</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(HRT_Stage__c, &apos;4 - Implementation Team I&apos;), 
ISPICKVAL(Status , &apos;Completed&apos;) , 
ISNULL(HRT_Cutover_Date__c) 
)</errorConditionFormula>
        <errorDisplayField>HRT_Cutover_Date__c</errorDisplayField>
        <errorMessage>Cutover Date is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_5_Implementation_Team_II</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(HRT_Stage__c, &apos;5 - Implementation Team II&apos;), 
ISPICKVAL(Status , &apos;Completed&apos;) , 
ISNULL(HRT_Cutover_process_completion_date__c) 
)</errorConditionFormula>
        <errorDisplayField>HRT_Cutover_process_completion_date__c</errorDisplayField>
        <errorMessage>Cutover Process Completion Date
is required</errorMessage>
    </validationRules>
