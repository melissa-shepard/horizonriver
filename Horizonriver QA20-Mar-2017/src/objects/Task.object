        <fullName>sendgrid4sf__Send_Grid</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Schedule Email via MassMailer</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var Cid;
var lid;
var aid;

Cid = &apos;{!Contact.Id}&apos;;

lid= &apos;{!Lead.Id}&apos;;

aid=&apos;{!Account.Id}&apos;;

var url;

 if(lid!=null &amp;&amp; Cid==&apos;&apos; &amp;&amp; aid==&apos;&apos;)
{

url=&quot;/apex/sendgrid4sf__send_email_sendgrid2?leadId=&quot;+lid;
}
else
{
 if (aid!=null &amp;&amp; Cid==&apos;&apos;)
      {
      
       url=&quot;/apex/sendgrid4sf__send_email_sendgrid2?accountId=&quot;+aid;
       }
     else
      {
      
       url=&quot;/apex/sendgrid4sf__send_email_sendgrid2?contactId=&quot;+Cid;
       }

    
}
self.location.href = url;</url>
    </webLinks>
