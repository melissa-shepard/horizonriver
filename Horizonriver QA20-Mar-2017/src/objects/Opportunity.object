        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)} 
var records = {!GETRECORDIDS($ObjectType.Opportunity)}; 
var updateRecords = []; 
if (records[0] == null) 
{ 
alert(&quot;Please select at least one Opportunity&quot;) 
} 
else 
{ 
for (var i=0; i&lt;records.length; i++) 
{ 
var opp = new sforce.SObject(&quot;Opportunity&quot;); 
opp.id = records[i]; 
opp.GenerateBilling__c = true; 
updateRecords.push(opp); 
} 
result = sforce.connection.update(updateRecords); 
